<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

	<!-- Context parameters -->

	<!-- Servlets -->
	<!--<servlet>
		<servlet-name>UserProfile</servlet-name>
		<servlet-class>edu.ycp.cs481.linkup.ui.servlet.UserProfileServlet</servlet-class>
	</servlet> -->
	<servlet>
		<servlet-name>CreateUser</servlet-name>
		<servlet-class>edu.ycp.cs481.linkup.ui.servlet.CreateUserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Index</servlet-name>
		<servlet-class>edu.ycp.cs481.linkup.ui.servlet.IndexServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LookingFor</servlet-name>
		<servlet-class>edu.ycp.cs481.linkup.ui.servlet.LookingForServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>NewUserProfile</servlet-name>
		<servlet-class>edu.ycp.cs481.linkup.ui.servlet.addProfileInfoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Match</servlet-name>
		<servlet-class>edu.ycp.cs481.linkup.ui.servlet.MatchServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ProfileView</servlet-name>
		<servlet-class>edu.ycp.cs481.linkup.ui.servlet.ProfileViewServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>MatchProfileView</servlet-name>
		<servlet-class>edu.ycp.cs481.linkup.ui.servlet.MatchProfileViewServlet</servlet-class>
	</servlet>
	


	<!-- Servlet-mapping -->
	<!--<servlet-mapping>
		<servlet-name>UserProfile</servlet-name>
		<url-pattern>/profile/*</url-pattern>
	</servlet-mapping>-->
	<servlet-mapping>
		<servlet-name>CreateUser</servlet-name>
		<url-pattern>/createUser/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Index</servlet-name>
		<url-pattern>/index/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LookingFor</servlet-name>
		<url-pattern>/lookingFor/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>NewUserProfile</servlet-name>
		<url-pattern>/SetUpProfileInfo/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Match</servlet-name>
		<url-pattern>/userMatch/*</url-pattern>
	</servlet-mapping> 
	
	
	<servlet-mapping>
		<servlet-name>ProfileView</servlet-name>
		<url-pattern>/userProfile/*</url-pattern>
	</servlet-mapping> 
	
	<servlet-mapping>
		<servlet-name>MatchProfileView</servlet-name>
		<url-pattern>/matchProfile/*</url-pattern>
	</servlet-mapping> 

	<welcome-file-list>
		<welcome-file>/index/*</welcome-file>
	</welcome-file-list>

	<!-- Use the /index servlet as the welcome file -->
	<!--
	<welcome-file-list>
		<welcome-file>/index</welcome-file>
	</welcome-file-list>
	-->
	

</web-app>